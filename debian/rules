#!/usr/bin/make -f

%:
	dh $@ --parallel

override_dh_auto_configure:
	./autogen.sh
	dh_auto_configure -- --enable-indicator=auto

override_dh_install:
	find debian/tmp -name *.la -delete
	find debian/tmp -name *.a -delete
	dh_install --fail-missing

override_dh_strip:
	dh_strip --dbg-package=indicator-synapse-dbg

